{
  "name": "Projectize",
  "description": "Project Oriented Browsing",
  "version": "0.0.1",
  "manifest_version": 2,
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage", "tabs",  "activeTab", "<all_urls>", "system.display"
  ]
}