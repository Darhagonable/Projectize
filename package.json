{
  "name": "projectize",
  "version": "1.0.0",
  "license": "MIT",
  "targets": {
    "webext-dev": {
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "webext-prod": {}
  },
  "scripts": {
