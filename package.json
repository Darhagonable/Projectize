    "build": "parcel build src/manifest.json --target webext-prod --no-cache"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@parcel/config-webextension": "2.0.0-beta.2",
    "@parcel/core": "2.0.0-beta.2",
    "parcel": "2.0.0-beta.2",
    "postcss": "^8.2.13"
  }
}
