{
  "name": "projectize",
  "version": "1.0.0",
  "targets": {
    "webext-dev": {
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "webext-prod": {}
  },
  "scripts": {
    "start": "parcel src/manifest.json --host localhost --target webext-dev --no-cache",
    "build": "parcel build src/manifest.json --target webext-prod --no-cache"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@parcel/config-webextension": "^2.0.0-nightly.2151",
    "@parcel/core": "^2.0.0-alpha.3",
    "parcel": "^2.0.0-beta.2",
    "parcel-bundler": "^1.12.5"
  }
}
